{
    "name": "sputnik-custom-resource-helper-utils",
    "description": "Utils for the app",
    "main": "libs.js",
    "author": {
        "name": "teuteuguy"
    },
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "moment": "2.24.0",
        "underscore": "1.10.2",
        "uuid": "8.0.0",
        "usage-metrics": "file:../usage-metrics/"
    },
    "scripts": {
        "build:init": "rm -rf dist && rm -rf node_modules",
        "build:zip": "zip -rq `jq -cr '.name' package.json`.zip .",
        "build:dist": "mkdir dist && mv `jq -cr '.name' package.json`.zip dist/",
        "build": "npm run build:init && npm install --production ---no-package-lock && npm run build:zip && npm run build:dist"
    }
}
