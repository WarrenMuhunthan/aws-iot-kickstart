{
    "name": "cdk",
    "version": "1.0.0",
    "bin": {
        "cdk": "bin/cdk.js"
    },
    "private": true,
    "scripts": {
        "bootstrap": "CDK_NEW_BOOTSTRAP=3 npx cdk bootstrap",
        "bootstrap:trust": "CDK_NEW_BOOTSTRAP=3 npx cdk bootstrap --trust `aws sts get-caller-identity --query Account --output text` --cloudformationExecutionPolicies arn:aws:iam::aws:policy/AdministratorAccess",
        "build": "tsc",
        "watch": "npm run clean && tsc -w",
        "test": "jest",
        "cdk": "npx cdk",
        "clean": "tsc --build --clean && rm -rf out cdk.out",
        "clean:deep": "npm run clean && find lib -type d -name node_modules -exec rm -rf {} \\;",
        "prod:pipeline:deploy": "npx cdk deploy --require-approval never Sputnik-Pipeline",
        "synth": "npx cdk synth",
        "dev:build": "npm run build",
        "dev:deploy:pipeline": "npx cdk deploy --require-approval never Sputnik-Pipeline",
        "dev:deploy:persistent": "npx cdk deploy --require-approval never Sputnik-Persistent",
        "dev:DESTROY:persistent": "npx cdk destroy Sputnik-Persistent",
        "dev:deploy:app": "npx cdk deploy --require-approval never Sputnik-App",
        "dev:deploy:app:force": "npx cdk deploy --require-approval never Sputnik-App --force",
        "dev:synth:app": "npx cdk synth Sputnik-App",
        "dev:DESTROY:app": "npx cdk destroy Sputnik-App"
    },
    "devDependencies": {
        "@aws-cdk/assert": "1.45.0",
        "@types/jest": "^24.0.22",
        "@types/node": "10.17.5",
        "aws-cdk": "1.45.0",
        "aws-sdk": "^2.675.0",
        "concurrently": "^5.2.0",
        "eslint-plugin-ordered-imports": "^0.2.3",
        "jest": "^24.9.0",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.1.0",
        "typescript": "~3.7.2"
    },
    "dependencies": {
        "@aws-cdk/app-delivery": "1.45.0",
        "@aws-cdk/assets": "1.45.0",
        "@aws-cdk/aws-apigateway": "1.45.0",
        "@aws-cdk/aws-appsync": "1.45.0",
        "@aws-cdk/aws-cloudformation": "1.45.0",
        "@aws-cdk/aws-cloudfront": "1.45.0",
        "@aws-cdk/aws-codepipeline": "1.45.0",
        "@aws-cdk/aws-codepipeline-actions": "1.45.0",
        "@aws-cdk/aws-cognito": "1.45.0",
        "@aws-cdk/aws-dynamodb": "1.45.0",
        "@aws-cdk/aws-iam": "1.45.0",
        "@aws-cdk/aws-iot": "1.45.0",
        "@aws-cdk/aws-kinesisfirehose": "1.45.0",
        "@aws-cdk/aws-lambda": "1.45.0",
        "@aws-cdk/aws-lambda-event-sources": "1.45.0",
        "@aws-cdk/aws-s3": "1.45.0",
        "@aws-cdk/aws-s3-assets": "1.45.0",
        "@aws-cdk/aws-s3-deployment": "1.45.0",
        "@aws-cdk/aws-sns": "1.45.0",
        "@aws-cdk/aws-sns-subscriptions": "1.45.0",
        "@aws-cdk/aws-ssm": "1.45.0",
        "@aws-cdk/core": "1.45.0",
        "@mobileposse/auto-delete-bucket": "^1.28.0",
        "@types/graphql": "14.0.7",
        "@types/ini": "^1.3.30",
        "@types/js-yaml": "^3.12.3",
        "@types/lodash": "^4.14.150",
        "@types/shortid": "0.0.29",
        "@types/smart-truncate": "^1.0.0",
        "appsync-schema-converter": "^0.1.3",
        "cdk-constants": "^3.0.3",
        "cdk-iam-actions": "^1.0.0",
        "chalk": "^4.0.0",
        "graphql": "^14.1.1",
        "graphql-tools": "^4.0.4",
        "hash-string": "^1.0.0",
        "ini": "^1.3.5",
        "lodash": "^4.17.15",
        "npm-cache": "^0.7.0",
        "short-hash": "^1.0.0",
        "shorthash": "0.0.2",
        "shortid": "^2.2.15",
        "smart-truncate": "^1.0.1",
        "source-map-support": "^0.5.16",
        "tree-cli": "^0.6.4",
        "yaml": "^1.9.2",
        "yaml-cfn": "^0.2.3"
    }
}
