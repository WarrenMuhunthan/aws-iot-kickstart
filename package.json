{
	"name": "aws-iot-kitstarter",
	"version": "1.0.0",
	"description": "The IoT Pilot Kickstart Solution",
	"main": "index.js",
	"private": true,
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"format": "prettier --write '**/*.{ts,js,json,yml}'",
		"format:check": "prettier --check '**/*.{ts,js,json,yml}'",
		"lint": "eslint --ext .js --ext .ts --ext .json --cache --ignore-path .gitignore .",
		"lint:fix": "eslint --ext .js --ext .ts --ext .json --cache --ignore-path .gitignore --fix .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean": "(cd cdk && npm run clean) && (cd source/console && npm run clean)",
		"clean:deep:packages": "(cd cdk && npm run clean:deep) && (cd source/console && npm run clean:deep)",
		"clean:deep": "npm run clean && npm run clean:deep:packages && find . -type d -d -iname node_modules -exec rm -rf \"{}\" \\;",
		"install:deep": "npm install && (cd cdk && npm install) && (cd source/console && npm install)"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/aws-samples/aws-iot-kickstart.git"
	},
	"author": "",
	"license": "Apache-2.0",
	"devDependencies": {
		"@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
		"@typescript-eslint/eslint-plugin": "^1.13.0",
		"@typescript-eslint/parser": "^1.13.0",
		"eslint": "^5.12.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-standard": "^12.0.0",
		"eslint-import-resolver-babel-module": "^5.0.0",
		"eslint-import-resolver-typescript": "^1.1.1",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jest": "^22.1.3",
		"eslint-plugin-json": "^1.3.2",
		"eslint-plugin-lodash": "^5.1.0",
		"eslint-plugin-node": "^8.0.1",
		"eslint-plugin-ordered-imports": "^0.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-promise": "^4.0.1",
		"eslint-plugin-standard": "^4.0.0",
		"prettier": "2.0.5",
		"ts-node": "^8.1.0",
		"typescript": "~3.7.2"
	},
	"dependencies": {}
}
